{
  "name": "@jipd/string-ucfirst",
  "version": "1.0.0",
  "description": "Capitalise the first letter of a string",
  "main": "dist/index.js",
  "directories": {
    "lib": "lib"
  },
  "scripts": {
    "test": "jest test/*",
    "clean": "rm -rf dist && mkdir dist",
    "build-babel": "babel ./lib -d ./dist -s",
    "build": "npm run clean && npm run build-babel",
    "dev": "watch 'npm run build' ./lib",
    "prepare": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jipd/string-ucfirst.git"
  },
  "keywords": [
    "javascript",
    "ucfirst"
  ],
  "author": "Nathan Bryant",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/jipd/string-ucfirst/issues"
  },
  "homepage": "https://github.com/jipd/string-ucfirst#readme",
  "devDependencies": {
    "@babel/cli": "^7.14.8",
    "@babel/plugin-transform-runtime": "^7.14.5",
    "@babel/preset-env": "^7.14.8",
    "babel-preset-minify": "^0.5.1",
    "eslint": "^7.31.0",
    "jest": "^27.0.6",
    "watch": "^1.0.2"
  }
}
